<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mingmin.materialdesign">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".bottomsheet.BottomSheetActivity" />
        <activity android:name=".transparenttoolbar.TransparentToolbarActivity" />
        <activity android:name=".ratingbar.RatingBarActivity" />
        <activity android:name=".firestore.FirestoreActivity" />
        <activity android:name=".mvvm.MvvmActivity" />
        <activity android:name=".firestore.FirestoreDetailActivity" />
    </application>

</manifest>